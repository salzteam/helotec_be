{
    "env": {
      "commonjs": true,
      "es2021": true,
      "node": true
    },
    // "extends": "eslint:recommended",
    "extends": ["prettier"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error", { "endOfLine": "auto" }]
    }
  }
  